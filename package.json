{
  "name": "ble-expo-config-plugin",
  "version": "1.0.0",
  "description": "Config plugin to auto configure ble permission on prebuild",
  "main": "build/withBLE.js",
  "types": "build/withBLE.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/M4nk1n/ble-expo-config-plugin.git"
  },
  "files": [
    "build",
    "app.plugin.js"
  ],
  "scripts": {
    "bootstrap": "pnpm install && pnpm build",
    "build": "tsc --build",
    "clean": "rm -rf build tsconfig.tsbuildinfo",
    "clean:lib": "rm -rf node_modules"
  },
  "keywords": [
    "react-native",
    "expo"
  ],
  "jest": {
    "preset": "expo-module-scripts"
  },
  "peerDependencies": {
    "expo": "^46.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@tsconfig/node16": "^1.0.0",
    "eslint": "^8.23.0",
    "prettier": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.0"
  }
}
